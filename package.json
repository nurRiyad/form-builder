{
  "name": "@nurriyad/form-builder",
  "version": "0.0.12",
  "private": false,
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/form-builder.umd.cjs",
  "module": "./dist/form-builder.js",
  "exports": {
    ".": {
      "import": "./dist/form-builder.js",
      "require": "./dist/form-builder.umd.cjs",
      "types": "./dist/src/lib.d.ts"
    },
    "./css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist-app && run-p type-check \"build-only {@}\" --",
    "build:lib": "rimraf dist && MODE=lib run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "rimraf dist-app && vite build",
    "build-only:lib": "rimraf dist && MODE=lib  vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "schema:gen": "ts-json-schema-generator --path 'src/types/index.ts' --type 'FormType' --out 'src/schema/index.json'",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "@vueuse/core": "^11.0.3"
  },
  "peerDependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@appscode/design-system": "^2.2.62",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "bulma": "^0.9.1",
    "bulma-checkradio": "^2.1.3",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.41",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.79.3",
    "start-server-and-test": "^2.0.4",
    "ts-json-schema-generator": "^2.3.0",
    "typescript": "~5.4.0",
    "typescript-json-schema": "^0.65.1",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitepress": "^1.3.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
